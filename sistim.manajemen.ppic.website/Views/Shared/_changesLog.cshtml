@model sistem.manajemen.ppic.website.Models.BaseModel

<div class="row">
    <div class="col-lg-12">
        <div class="ibox ">
            <div class="ibox-title">
                <h5>History</h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <table id="Tablelog" class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>Date</th>
                                    <th>User</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var data in Model.ChangesHistory)
                                {
                                    <tr>
                                        <td>@Html.Raw(data.MODIFIED_DATE.ToString("dd-MMM-yyyy HH:mm:ss"))</td>
                                        <td>@Html.Raw(data.MODIFIED_BY)</td>
                                        <td>@Html.Raw(data.ACTION)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
