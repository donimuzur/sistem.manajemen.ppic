    <!-- Start Left menu area -->
    <div class="left-sidebar-pro">
        <nav id="sidebar" class="">
            <div class="sidebar-header" style="border-bottom:5px solid black;">
                <a href="@Url.Action("Index","Home")"><img class="main-logo" src="~/Content/img/logo/logo.png" alt="" /></a>
                <strong><a href="@Url.Action("Index","Home")"><img src="~/Content/img/logo/logosn.png" alt="" /></a></strong>
            </div>
            <div class="left-custom-menu-adp-wrap comment-scrollbar">
                <nav class="sidebar-nav left-sidebar-menu-pro">
                    <ul class="metismenu" id="menu1">
                        <li id="transaksi">
                            <a class="has-arrow" title="Transaski">
                                <span class="fa fa-file-o icon-wrap"></span>
                                <span class="mini-click-non">Transaksi</span>
                            </a>
                            <ul class="submenu-angle" aria-expanded="false">
                                <li><a title="SPB" href="index.html"  id="transaksi_SPB"><span class="mini-sub-pro">SPB</span></a></li>
                                <li><a title="DO" href="index-1.html"  id="transaksi_DO"><span class="mini-sub-pro">DO</span></a></li>
                                <li><a title="Pengiriman" href="index-2.html"  id="transaksi_Pengiriman"><span class="mini-sub-pro">Pengiriman</span></a></li>
                            </ul>
                        </li>
                        <li  id="gudang">
                            <a class="has-arrow" title="Gudang">
                                <span class="fa fa-cubes icon-wrap"></span>
                                <span class="mini-click-non">Gudang</span>
                            </a>
                            <ul class="submenu-angle" aria-expanded="false">
                                <li><a title="Barang Jadi" href="@Url.Action("Index","MstBarangJadi")"  id="gudang_barangjadi"><span class="mini-sub-pro">Barang Jadi</span></a></li>
                                <li><a title="Barang Bahan Baku" href="#"><span class="mini-sub-pro" id="gudang_barangbb">Barang BB</span></a></li>
                            </ul>
                        </li>
                        <li  id="master">
                            <a class="has-arrow" title="Master">
                                <span class="fa fa-align-justify icon-wrap"></span>
                                <span class="mini-click-non">Master</span>
                            </a>
                            <ul class="submenu-angle" aria-expanded="false">
                                <li><a title="Master Wilayah" href="#"  id="master_wilayah"><span class="mini-sub-pro">Master Wilayah</span></a></li>
                                <li><a title="Master Konsumen" href="#"  id="master_konsumen"><span class="mini-sub-pro">Master Konsumen</span></a></li>
                                <li><a title="Master Transport" href="#" id="master_transport"><span class="mini-sub-pro">Master Transport</span></a></li>
                            </ul>
                        </li>
                        <li  id="laporan">
                            <a class="has-arrow" title="Laporan">
                                <span class="fa fa-file-excel-o icon-wrap"></span>
                                <span class="mini-click-non">Laporan</span>
                            </a>
                            <ul class="submenu-angle" aria-expanded="false">
                                <li><a title="Laporan Rekap Bulanan" href="index.html" id="laporan_rekapbulanan"><span class="mini-sub-pro">Laporan Rekap Bulanan</span></a></li>
