                    <div class="logo-pro" style="margin-bottom:50px;">
                        @*<a href="index.html"><img class="main-logo" src="~/Content/img/logo/logo.png" alt="" /></a>*@
                    </div>
                </div>
            </div>
        </div>
        <div class="header-advance-area">
            <div class="header-top-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="header-top-wraper">
                                <div class="row">
                                    <div class="col-lg-1 col-md-0 col-sm-1 col-xs-12">
                                        <div class="menu-switcher-pro">
                                            <button type="button" id="sidebarCollapse" class="btn bar-button-pro header-drl-controller-btn btn-info navbar-btn">
                                                <i class="educate-icon educate-nav"></i>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="col-lg-11 col-md-11 col-sm-11 col-xs-11">
                                        <div class="header-right-info" style="float:right;">
                                            <ul class="nav navbar-nav mai-top-nav header-right-menu">
                                                <li class="nav-item">
                                                    <a href="#" data-toggle="dropdown" role="button" aria-expanded="false" class="nav-link dropdown-toggle">
                                                        <img src="~/Content/img/product/no-pict.jpg" alt="" />
                                                        @{
                                                            var Name = Model.CurrentUser.FIRST_NAME + " " + Model.CurrentUser.LAST_NAME;
                                                        }
                                                        <span class="admin-name">@Name</span>
                                                        <i class="fa fa-angle-down edu-icon edu-down-arrow"></i>
                                                    </a>
                                                    <ul role="menu" class="dropdown-header-top author-log dropdown-menu animated zoomIn">
                                                        <li>
                                                            <a href="@Url.Action("SignOut", "Login")"><span class="edu-icon edu-locked author-log-ic"></span>Log Out</a>
                                                        </li>
                                                    </ul>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Mobile Menu start -->
            <div class="mobile-menu-area">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="mobile-menu">
                                <nav id="dropdown">
                                    <ul class="mobile-menu-nav">
                                        <li>
                                            <a data-toggle="collapse">Transaksi<span class="admin-project-icon edu-icon edu-down-arrow"></span></a>
                                            <ul class="collapse dropdown-header-top">
                                                <li><a href="#">SPB</a></li>
                                                <li><a href="#">DO</a></li>
                                                <li><a href="#">Pengiriman</a></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a data-toggle="collapse">Gudang<span class="admin-project-icon edu-icon edu-down-arrow"></span></a>
                                            <ul class="collapse dropdown-header-top">
                                                <li><a href="@Url.Action("Index","MstBarangJadi")">Barang Jadi</a></li>
                                                <li><a href="#">Barang BB</a></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a data-toggle="collapse">Master<span class="admin-project-icon edu-icon edu-down-arrow"></span></a>
                                            <ul class="collapse dropdown-header-top">
                                                <li><a href="#">Master Wilayah</a></li>
                                                <li><a href="#">Master Konsumen</a></li>
                                                <li><a href="#">Master Transport</a></li>
                                            </ul>
                                        </li>
                                        <li>
                                            <a data-toggle="collapse">Laporan<span class="admin-project-icon edu-icon edu-down-arrow"></span></a>
                                            <ul class="collapse dropdown-header-top">
                                                <li><a href="#">Laporan Rekap Bulanan</a></li>
                                                <li><a href="#">Laporan Outstanding</a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Mobile Menu end -->
            <div class="breadcome-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="breadcome-list">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <div class="sparkline13-hd">
                                            <div class="main-sparkline13-hd">
                                                <h1>@Model.Menu</h1>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">

                                        <ul class="breadcome-menu">
                                            <li>
                                                <a href="@Url.Action("Index","Home")">Home</a> 
                                            </li>
                                            @if(!string.IsNullOrEmpty(Model.Menu))
                                            {
                                                <li>
                                                    <span class="bread-slash">/</span><span class="bread-blod">@Model.Menu</span>
                                                </li>
                                            }
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @RenderBody()
        <div class="footer-copyright-area">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="footer-copy-right">
                            <p>Copyright © 2018. All rights reserved</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- jquery
        ============================================ -->
